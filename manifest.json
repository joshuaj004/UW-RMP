{
  "manifest_version": 2,

  "name": "UW Rate My Professor Checker",
  "description": "This extension returns the rating for a UW Professor",
  "version": "0.0.2",
  "background": {
          "scripts": ["popup.js"],
          "persistent": false
      },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
